{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "type": {
            "type": "string"
        },
        "shcemaLocation": {
            "type": "string"
        },
        "schemaVersion": {
            "type": "string"
        },
        "generator": {
            "type": "object",
            "properties": {
                "app": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "generatedAt": {
                    "type": "string"
                }
            },
            "required": [
                "app",
                "version",
                "generatedAt"
            ]
        },
        "content": {
            "type": "object",
            "properties": {
                "publisher": {
                    "type": "string"
                },
                "publishedAt": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "text": {
                    "type": "string"
                }
            },
            "required": [
                "publisher",
                "publishedAt",
                "text"
            ]
        }
    },
    "required": [
        "type",
        "shcemaLocation",
        "schemaVersion",
        "generator",
        "content"
    ]
}